<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <FrameLayout
        android:id="@+id/inner_frameview"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/fragment_main_navigation"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/fragment_main_navigation"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/light_white"
        app:itemIconTint="@drawable/bottom_menu_icon_selector"
        app:itemTextColor="@drawable/bottom_menu_icon_selector"
        app:layout_constraintBottom_toBottomOf="parent"
        app:menu="@menu/tab_menu" />


</androidx.constraintlayout.widget.ConstraintLayout>